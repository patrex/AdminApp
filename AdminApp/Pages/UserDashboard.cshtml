@page
@model AdminApp.Pages.UserDashboardModel
@{
    ViewData["Title"] = "UserDashboard";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<br />

<div class="container row">

    <div class="col-9">
        <h2 class="text-info">Hello, @Model.CurrentUser.ToString</h2>
    </div>

    <div class="col-3">
        <a asp-page="ReqItem" asp-route-id="@Model.CurrentUser.eMail" class="btn btn-info form-control text-white">Make Request</a>
    </div>

</div>

